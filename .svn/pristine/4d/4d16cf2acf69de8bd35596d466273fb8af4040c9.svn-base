<UserControl x:Class="TypApplication.View.Popups.CourseReadyPopup"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:TypApplication.View.Popups"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="380">
    <Grid Margin="0,0,10,10" Background="#FFBDC3C7">
        <Ellipse Fill="#FFE74C3C" HorizontalAlignment="Left" Height="31" Margin="330,10,0,0" Stroke="#FF303030" VerticalAlignment="Top" Width="31" RenderTransformOrigin="9.762,0.476"/>
        <Label Content="There is a Course ready to make" HorizontalAlignment="Left" Margin="50,60,0,0" VerticalAlignment="Top" FontSize="20" Foreground="#FF2C3E50"/>
        <Button Content="Start" HorizontalAlignment="Left" Margin="103,117,0,0" VerticalAlignment="Top" Width="187" Height="50" Background="#FF2ECC71" Foreground="#FFECF0F1" FontSize="20" BorderBrush="#FF00FF6C"/>
    </Grid>
</UserControl>
